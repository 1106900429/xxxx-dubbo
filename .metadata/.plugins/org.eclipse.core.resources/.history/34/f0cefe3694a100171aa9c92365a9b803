package com.xxxx.controller;

import javax.ws.rs.QueryParam;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.PathVariable;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RequestMethod;

import com.xxxx.mybatis.domain.Enterprise;
import com.xxxx.repo.IEnterpriseService;

@Controller
@RequestMapping(value="/enterprise")
public class EnterpriseController {
	
	
	@Autowired
	IEnterpriseService enterpriseService;
	
    @RequestMapping(value="/{enterpriseId}",method=RequestMethod.GET)
	public Enterprise getEnterpriseById(@PathVariable int enterpriseId) {
		// TODO Auto-generated method stub
		System.out.println("sdfsfdsfsdfsdf");
		return enterpriseService.getEnterpriseById(enterpriseId);
	}

}
